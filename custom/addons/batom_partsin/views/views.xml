        <record id="view_shop_in_form" model="ir.ui.view">
            <field name="name">batom.shop_in.form</field>
            <field name="model">batom.shop_in</field>
            <field name="arch" type="xml">
                <form string="Shop In">
                    <sheet>
                        <group>
                            <group>
                                <field name="pdate"/>
                                <field name="supplier" domain="[('supplier','=',True)]" context="{'search_default_supplier':1}"/>
                                <field name="customer"/>
                                <field name="prodid"/>
                                <field name="processid"/>
                                <field name="process"/>
                                <field name="inqty"/>
                                <field name="nextprcs"/>
                                <field name="batch"/>
                            </group>
                            <group>
                                <field name="mkordid"/>
                                <field name="creator"/>
                                <field name="urgency"/>
                                <field name="location"/>
                                <field name="pnote"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Factory">
                                <group>
                                    <group>
                                        <field name="sprogress"/>
                                        <field name="printerdata"/>
                                        <field name="sdate"/>
                                        <field name="outqty"/>
                                        <field name="ngqty"/>
                                        <field name="mfbatch"/>
                                    </group>
                                    <group>
                                        <field name="receiver"/>
                                        <field name="snote"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Miscellaneous">
                                <group>
                                    <group>
                                        <field name="parts_in_id"/>
                                        <field name="indate"/>
                                        <field name="qrinfo"/>
                                        <field name="finish"/>
                                        <field name="transfer"/>
                                        <field name="transtime"/>
                                    </group>
                                    <group>
                                        <field name="origin_id"/>
                                        <field name="lastdate"/>
                                        <field name="lastperson"/>
                                    </group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

